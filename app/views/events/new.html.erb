<h1>Events#new</h1>

<%= form_for @event do |f| %>

    <%= f.label :name, "Store Name" %>
    <%= f.text_field :name %>
    
    <br>
    
    <%= f.label :location %>
    <%= f.text_field :location %>

    <br>
    
    <%= f.label :date %>
    <%= f.text_field :date %>
    
    <br>
    
    <%= f.label :description %>
    <%= f.text_area :description %>

    <br>
    
    <%= f.submit %>
    
<% end %>